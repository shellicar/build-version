{
  "name": "@shellicar/build-version",
  "private": false,
  "version": "1.3.0",
  "type": "module",
  "license": "MIT",
  "author": "Stephen Hellicar",
  "description": "Build plugin that calculates and exposes version information through a virtual module import.",
  "keywords": [
    "build",
    "plugin",
    "version",
    "esbuild",
    "vite",
    "gitversion",
    "git"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shellicar/build-version.git"
  },
  "bugs": {
    "url": "https://github.com/shellicar/build-version/issues"
  },
  "homepage": "https://github.com/shellicar/build-version#readme",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./version": {
      "require": {
        "types": "./dist/cjs/core/export.d.cts",
        "default": "./dist/cjs/core/export.cjs"
      },
      "import": {
        "types": "./dist/esm/core/export.d.ts",
        "default": "./dist/esm/core/export.js"
      }
    },
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./astro": {
      "require": {
        "types": "./dist/cjs/astro.d.cts",
        "default": "./dist/cjs/astro.cjs"
      },
      "import": {
        "types": "./dist/esm/astro.d.ts",
        "default": "./dist/esm/astro.js"
      }
    },
    "./rspack": {
      "require": {
        "types": "./dist/cjs/rspack.d.cts",
        "default": "./dist/cjs/rspack.cjs"
      },
      "import": {
        "types": "./dist/esm/rspack.d.ts",
        "default": "./dist/esm/rspack.js"
      }
    },
    "./vite": {
      "require": {
        "types": "./dist/cjs/vite.d.cts",
        "default": "./dist/cjs/vite.cjs"
      },
      "import": {
        "types": "./dist/esm/vite.d.ts",
        "default": "./dist/esm/vite.js"
      }
    },
    "./webpack": {
      "require": {
        "types": "./dist/cjs/webpack.d.cts",
        "default": "./dist/cjs/webpack.cjs"
      },
      "import": {
        "types": "./dist/esm/webpack.d.ts",
        "default": "./dist/esm/webpack.js"
      }
    },
    "./rollup": {
      "require": {
        "types": "./dist/cjs/rollup.d.cts",
        "default": "./dist/cjs/rollup.cjs"
      },
      "import": {
        "types": "./dist/esm/rollup.d.ts",
        "default": "./dist/esm/rollup.js"
      }
    },
    "./esbuild": {
      "require": {
        "types": "./dist/cjs/esbuild.d.cts",
        "default": "./dist/cjs/esbuild.cjs"
      },
      "import": {
        "types": "./dist/esm/esbuild.d.ts",
        "default": "./dist/esm/esbuild.js"
      }
    },
    "./nuxt": {
      "require": {
        "types": "./dist/cjs/nuxt.d.cts",
        "default": "./dist/cjs/nuxt.cjs"
      },
      "import": {
        "types": "./dist/esm/nuxt.d.ts",
        "default": "./dist/esm/nuxt.js"
      }
    },
    "./farm": {
      "require": {
        "types": "./dist/cjs/farm.d.cts",
        "default": "./dist/cjs/farm.cjs"
      },
      "import": {
        "types": "./dist/esm/farm.d.ts",
        "default": "./dist/esm/farm.js"
      }
    },
    "./types": {
      "require": {
        "types": "./dist/cjs/types.d.cts",
        "default": "./dist/cjs/types.cjs"
      },
      "import": {
        "types": "./dist/esm/types.d.ts",
        "default": "./dist/esm/types.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "typesVersions": {
    "*": {
      "version": [
        "./dist/esm/core/export.d.ts"
      ],
      "astro": [
        "./dist/esm/astro.d.ts"
      ],
      "rspack": [
        "./dist/esm/rspack.d.ts"
      ],
      "vite": [
        "./dist/esm/vite.d.ts"
      ],
      "webpack": [
        "./dist/esm/webpack.d.ts"
      ],
      "rollup": [
        "./dist/esm/rollup.d.ts"
      ],
      "esbuild": [
        "./dist/esm/esbuild.d.ts"
      ],
      "nuxt": [
        "./dist/esm/nuxt.d.ts"
      ],
      "farm": [
        "./dist/esm/farm.d.ts"
      ],
      "types": [
        "./dist/esm/types.d.ts"
      ],
      "index": [
        "./dist/esm/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "test": "vitest run",
    "type-check": "tsc -p tsconfig.check.json",
    "prepublishOnly": "run-s build test ci",
    "postpublish": "pnpm version --no-git-tag-version patch"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": "^6",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nuxt/kit": "^4.0.3",
    "@nuxt/schema": "^4.0.3",
    "@shellicar/build-clean": "1.0.0-preview.5",
    "@tsconfig/node20": "^20.1.6",
    "@types/node": "^24.3.0",
    "bumpp": "^10.2.3",
    "chalk": "^5.6.0",
    "esno": "^4.8.0",
    "npm-run-all2": "^8.0.4",
    "rollup": "^4.48.0",
    "terser": "^5.43.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "webpack": "^5.101.3"
  },
  "dependencies": {
    "unplugin": "^2.3.8"
  }
}
