{
  "extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "module": "ES2022",
    "noEmit": true,
    "skipLibCheck": true,
    "composite": true,
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
